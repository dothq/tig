{
    "name": "Dot Browser Test",
    "author": "Dot HQ",
    "id": "co.dothq.browsertest",
    "builder": "mach",
    "requires": ["hg", "git", "nonexistantpackage"],
    "targets": {
        "base": {
            "tag": ["venkman"],
            "name": "Mozilla Venkman",
            "http": "https://hg.mozilla.org/venkman",
            "repository": {
                "branch": "default"
            },
            "type": "mercurial"
        },
        "patch": {
            "tag": ["dot", "dotbrowser", "dothq", "db"],
            "name": "Dot Browser Stable",
            "http": "https://hg.mozilla.org/venkman",
            "repository": {
                "branch": "main"
            },
            "type": "mercurial"
        }
    },
    "flags": [
        "--enable-artifact-mode"
    ]
}